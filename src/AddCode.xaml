<Controls:MetroWindow x:Class="gauthUI.AddCodeWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
		Title="Add Code Manually" Height="157" Width="330">
	<Grid>
		<Label Content="Name" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="_nameLabel" VerticalAlignment="Top" />
		<Label Content="Key" Height="28" HorizontalAlignment="Left" Margin="12,47,0,0" Name="_keyLabel" VerticalAlignment="Top" />
		<TextBox Height="23" HorizontalAlignment="Left" Margin="75,14,0,0" Name="_nameTxt" VerticalAlignment="Top" Width="217" />
		<TextBox Height="23" HorizontalAlignment="Left" Margin="75,47,0,0" Name="_keyTxt" VerticalAlignment="Top" Width="217" />
		<Button Content="Add" Height="23" HorizontalAlignment="Left" Margin="136,86,0,0" Name="_addBtn" VerticalAlignment="Top" Width="75" Click="AddBtn_Click" IsDefault="true"/>
		<Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="217,86,0,0" Name="_cancelBtn" VerticalAlignment="Top" Width="75" IsCancel="true"/>
	</Grid>
</Controls:MetroWindow>
